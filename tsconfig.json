{
    "compilerOptions": {
      "jsx": "react"
    },
    "extends": "@tsconfig/react-native/tsconfig.json",
    "exclude": ["__tests__/**/*-test.ts"]

    "vueCompilerOptions": {
      "experimentalAdditionalLanguageModules": [true]
